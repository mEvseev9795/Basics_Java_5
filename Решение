public class Main {
    public static void main(String[] args) {
        
        Book book = new Book();
        book.welcome("Привет");
        book.name = "Совы охотятся ночью";
        book.author = "Энтони Горовиц";
        book.numberOfPages = 350;
        book.timeToRead = 170;
        
        System.out.println(book);
      
        Book book2 = new Book();
        book2.name = "Пес по имени Мани";
        book2.author = "Бодо Шефер";
        book2.numberOfPages = 85;
        book2.timeToRead = 40;
        
        System.out.println(book2); 
        
           System.out. println ("Общее время чтения " + (book.timeToRead + book2.timeToRead) + " мин.");
        
        System.out.println("Приятного чтения!");
    }
}
